{
  "schemaVersion": 1,
  "id": "fabrizoom",
  "version": "$version",
  "name": "FabriZoom",
  "description": "Adds a highly configurable zoom button. Fork of OkZoomer v4",
  "authors": [
    "EnnuiL",
    {
      "name": "trainb0y",
      "contact": {
        "email": "trainb0ydev@gmail.com",
        "discord": "trainb0y"
      }
    }
  ],
  "contact": {
    "issues": "https://github.com/trainb0y/FabriZoom/issues",
    "sources": "https://github.com/trainb0y/FabriZoom"
  },
  "license": "MIT",
  "icon": "assets/fabrizoom/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "io.github.trainb0y.fabrizoom.FabriZoom"
      }
    ],
    "modmenu": [
      "io.github.trainb0y.fabrizoom.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "fabrizoom.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric-language-kotlin": ">=1.10.10+kotlin.1.9.10",
    "fabric-key-binding-api-v1": "*",
    "yet_another_config_lib_v3": "3.*.*"
  },
  "recommends": {
    "modmenu": "7.*.*",
    "minecraft": "1.20.*"
  }
}
