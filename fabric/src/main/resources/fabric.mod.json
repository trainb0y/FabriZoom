{
  "schemaVersion": 1,
  "id": "fabrizoom",
  "version": "$version",
  "name": "FabriZoom",
  "description": "Lightweight, cross-platform, configurable zoom. Fork of OkZoomer v4",
  "authors": [
    "trainb0y",
    "EnnuiL"
  ],
  "contact": {
    "issues": "https://github.com/trainb0y/FabriZoom/issues",
    "sources": "https://github.com/trainb0y/FabriZoom"
  },
  "license": "MIT",
  "icon": "assets/fabrizoom/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "io.github.trainb0y.fabrizoom.fabric.FabricEntrypoint"
      }
    ],
    "modmenu": [
      "io.github.trainb0y.fabrizoom.fabric.ModMenuIntegration"
    ]
  },
  "mixins": [
    "fabrizoom.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15",
    "fabric-language-kotlin": ">=1.11.0+kotlin.2.0.0",
    "fabric-key-binding-api-v1": "*",
    "yet_another_config_lib_v3": "3.*.*"
  },
  "recommends": {
    "modmenu": "11.*.*",
    "minecraft": "1.21"
  }
}
